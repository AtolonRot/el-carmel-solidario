{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "_assets": "./src/assets",
          "_navigations": "./src/navigations",
          "_scenes": "./src/scenes",
          "_styles": "./src/styles",
        }
      }
    ]
  ]
}
